{
	"name": "@gylfie/cli",
	"version": "0.0.5",
	"type": "commonjs",
	"description": "Gylfie Javascript Command Line Interface",
	"main": "./bin/main.js",
	"scripts": {
		"prebuild": "rimraf bin",
		"build": "rimraf bin && tsc && gulp execute",
		"build:watch": "tsc --watch",
		"publish": "npm run build && npm run global",
		"refresh": "npm uninstall -g @gylfie/cli && npm install -g",
		"start": "node bin/main.js",
		"global": "npm i -g",
		"test": "jest",
		"lint": "eslint \"{src,apps,lib,test}/**/*.ts\" --fix",
		"format": "prettier --write .",
		"co:login": "aws codeartifact login --domain gylfie --repository gylfie --tool npm --profile GitauGakwaOwl --namespace @gylfie"
	},
	"keywords": [
		"gylfie"
	],
	"bin": {
		"gylfie": "./bin/main.js"
	},
	"devDependencies": {
		"@types/gulp": "^4.0.9",
		"@types/inquirer": "^8.2.1",
		"@types/lodash": "^4.14.182",
		"@types/luxon": "^2.3.2",
		"@types/node": "^14.14.41",
		"@types/uuid": "^8.3.4",
		"cross-env": "^7.0.3",
		"gulp": "^4.0.2",
		"gulp-chmod": "^3.0.0",
		"gulp-inject-string": "^1.1.2",
		"rimraf": "^3.0.2",
		"ts-node": "^10.7.0",
		"typescript": "^4.6.4",
		"tslib": "^2.4.0",
		"@types/cli-progress": "^3.9.2",
		"@types/adm-zip": "^0.5.0",
		"prettier": "^2.6.2"
	},
	"dependencies": {
		"commander": "^9.2.0",
		"dotenv": "^10.0.0",
		"inquirer": "^8.2.4",
		"jose": "^4.8.1",
		"lodash": "^4.17.21",
		"luxon": "^2.3.2",
		"reflect-metadata": "^0.1.13",
		"rxjs": "^7.x",
		"uuid": "^8.3.2",
		"winston": "^3.7.2",
		"winston-transport": "^4.5.0",
		"cli-progress": "^3.11.0",
		"axios": "^0.27.2",
		"adm-zip": "^0.5.9"
	},
	"bundledDependencies": [
		"fs-extra",
		"minimatch",
		"ignore"
	],
	"author": {
		"name": "Gylfie",
		"url": "https://gylfie.com",
		"organization": true
	},
	"license": "ISC"
}
